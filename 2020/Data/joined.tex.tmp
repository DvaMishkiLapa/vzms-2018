\vzmstitle{О СОБСТВЕННЫХ ЗНАЧЕНИЯХ ОДНОЙ БЕСКОНЕЧНОЙ МАТРИЦЫ}
\vzmsauthor{Гаркавенко}{Г.\,В.}
\vzmsauthor{Усков}{Д.\,Г.}
\vzmsinfo{Воронеж; {\it g.garkavenko@mail.ru}; {\it uskov.dan@mail.ru}}
