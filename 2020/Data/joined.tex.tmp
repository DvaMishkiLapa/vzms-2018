\vzmstitle{АНАЛИТИЧЕСКИЕ РЕШЕНИЯ ЗАДАЧИ ДЛЯ ВЫРОЖДЕНИЯ СИСТЕМЫ В ЧАСТНЫХ ПРОИЗВОДНЫХ}
\vzmsauthor{Зубова}{С.\,П.}
\vzmsauthor{Мохамад}{А.\,Х.}
\vzmsinfo{Воронеж, ВГУ; {\it spzubova@mail.ru}; {\it abdulftah.hosni90@gmail.com}}
